<!DOCTYPE html>
<html>
  <head>
    <title>Deportes</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'panel', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'panel', 'data-turbolinks-track': 'reload' %>
    
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
  </head>

  <body class="hold-transition skin-blue sidebar-mini">
  
    <div class="wrapper">

      <%= render 'panel/shared/header' %>
      
      <%= render 'panel/shared/nav' %>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Simple Tables
            <small>preview of simple tables</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Tables</a></li>
            <li class="active">Simple</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">

          <%= render 'panel/shared/info' %>

          <%= yield %>  
          
        </section>
        <!-- /.content -->
      </div>
      <!-- /.content-wrapper -->
      
      <%= render 'panel/shared/footer' %>

    </div>
<!-- ./wrapper -->

</body>

</html>